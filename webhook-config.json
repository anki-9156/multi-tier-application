{
  "webhook": {
    "name": "ecommerce-webhook",
    "targetPipeline": "ecommerce-codepipeline-pipeline",
    "targetAction": "SourceAction",
    "targetPipelineVersion": 1,
    "filters": [
      {
        "jsonPath": "$.ref",
        "matchEquals": "refs/heads/aws-code-pipeline"
      }
    ],
    "authentication": "GITHUB_HMAC",
    "authenticationConfiguration": {
      "SecretToken": "REPLACE_WITH_YOUR_GITHUB_TOKEN"
    },
    "registerWithThirdParty": true
  }
} 